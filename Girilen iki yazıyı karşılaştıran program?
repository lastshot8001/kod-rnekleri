cümle1 = str(input("1. cümleyi girin = "))
cümle2 = str(input("2. cümleyi girin = "))
cevap = 0
if len(cümle1) == len(cümle2):
    for i in range(len(cümle1)):
        if cümle1[i] != cümle2[i]:
            cevap = 1
  
else:
    cevap = 1
if cevap == 0:
    print("cümleler eşit")
else:
    print("cümleler eşit değil")
